                  [30.13179864025168, -29.653647930061226],
                  [30.44490899181418, -29.653647930061226],
                  [30.44490899181418, -29.388955781588702]]], null, false),
            {
              "system:index": "1"
            })]),
    AOI = ee.FeatureCollection("projects/ee-cecilmasemola/assets/AOI");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
// // Random Forest Supervised Classification with Sentinel
// var dataset = ee.FeatureCollection("FAO/GAUL/2015/level2");
// var UMGD= dataset.filter(ee.Filter.eq('ADM2_NAME', 'Umgungundlovu District Municipality'));
// Map.addLayer(UMGD,{color: 'blue'}, 'UMGD');
// Map.centerObject(UMGD, 8);

// Load Sentinel data
var image = ee.ImageCollection('COPERNICUS/S2_SR')
.filterDate('2022-04-01', '2022-04-30')
.filter(ee.Filter.lt('CLOUDY_PIXEL_PERCENTAGE', 20))
.filterBounds(roi)
.median();

var visParamsTrue = {bands: ['B8', 'B3', 'B2'], min: 0, max: 2500, gamma: 1.1};
Map.addLayer(image.clip(roi), visParamsTrue, "Sentinel 2022_clipped");
Map.centerObject(roi, 8);

// Import/collect training data
// var training = ee.FeatureCollection('');
// print(training);

///collected training data
var training  = water.merge(urban).merge(forest).merge(barren);
print(training, 'Merged Classes');

// This property stores the land cover labels as consecutive
// integers starting from zero.

// CART Supervised Classification with Sentinel

// integers starting from zero.
var label = 'Class';
var bands = ['B2', 'B3', 'B4', 'B8']; // These are bands with 10 meter spatial resolution. 
var input = image.select(bands);

// Overlay the points on the imagery to get training.
var trainImage = input.sampleRegions({
  collection: training,
  properties: [label],
  scale: 10
});

var trainingData = trainImage.randomColumn();
var trainSet = trainingData.filter(ee.Filter.lessThan('random', 0.8));
var testSet = trainingData.filter(ee.Filter.greaterThanOrEquals('random', 0.8));

// Classification Model
var classifier = ee.Classifier.smileCart().train(trainSet, label, bands);

// Classify the image
var classified = input.classify(classifier);
print(classified.getInfo());

// Define a palette for the classification.
var landcoverPalette = [
  'Blue', //water (0)
  'Red', //urban (1)
  'Green', //forest (2)
  'Yellow', //barren (4)
];

Map.addLayer(classified.clip(roi), {palette: landcoverPalette, min: 1, max: 5}, 'CARTclassification');
/////////////////////////////////////////////////////////////////////////////////////////////////////////////
// Accuracy Assessment
//Classify the testingSet and get a confusion matrix.
var confusionMatrix = ee.ConfusionMatrix(testSet.classify(classifier)
    .errorMatrix({
      actual: 'Class', 
      predicted: 'classification'
    }));

print('Confusion matrix:', confusionMatrix);
print('Overall Accuracy:', confusionMatrix.accuracy());
print('Producers Accuracy:', confusionMatrix.producersAccuracy());
print('Consumers Accuracy:', confusionMatrix.consumersAccuracy());

// Export classified map to Google Drive
Export.image.toDrive({
  image: classified,
  description: 'Sentinel_2_Classified_CART',
  scale: 20,
  region: roi,
  maxPixels: 1e13,
});

///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

//Random forest

var label = 'Class';
var bands = ['B2', 'B3', 'B4', 'B8']; // These are bands with 10 meter spatial resolution. 
var input = image.select(bands);

// Overlay the points on the imagery to get training.
var trainImage = input.sampleRegions({
  collection: training,
  properties: [label],
  scale: 10
});

var trainingData = trainImage.randomColumn();
var trainSet = trainingData.filter(ee.Filter.lessThan('random', 0.8));
var testSet = trainingData.filter(ee.Filter.greaterThanOrEquals('random', 0.8));

// Make a Random Forest classifier and train it.
var classifier = ee.Classifier.smileRandomForest(10)
     .train({
       features: trainSet,
       classProperty: label,
       inputProperties: bands
     });

// Classify the image
var classified = input.classify(classifier);
print(classified.getInfo());

// Define a palette for the classification.
var landcoverPalette = [
  'Blue', //water (0)
  'Red', //urban (1)
  'Green', //forest (2)
  'Yellow', //barren (4)
];
Map.addLayer(classified.clip(roi), {palette: landcoverPalette, min: 0, max:4 }, 'RFclassification');


///////////////////////////////////////////////////////////////////////////
//predictor importance
//var bandNames = image.bandNames();
var classifier = ee.Classifier.smileRandomForest(5).setOutputMode('MULTIPROBABILITY').train(trainSet,"Class",bands);
 
var dict = classifier.explain();
print('Explain:',dict);

var variable_importance = ee.Feature(null, ee.Dictionary(dict).get('importance'));

var chart =
  ui.Chart.feature.byProperty(variable_importance)
    .setChartType('ColumnChart')
    .setOptions({
      title: 'Random Forest Variable Importance',
      legend: {position: 'none'},
      hAxis: {title: 'Bands'},
      vAxis: {title: 'Importance'}
    });


print(chart); 
 
//////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// Accuracy Assessment
//Classify the testingSet and get a confusion matrix.
var confusionMatrix = ee.ConfusionMatrix(testSet.classify(classifier)
    .errorMatrix({
      actual: 'Class', 
      predicted: 'classification'
    }));

print('Confusion matrix:', confusionMatrix);
print('Overall Accuracy:', confusionMatrix.accuracy());
print('Producers Accuracy:', confusionMatrix.producersAccuracy());
print('Consumers Accuracy:', confusionMatrix.consumersAccuracy());

// Export classified map to Google Drive
Export.image.toDrive({
  image: classified,
  description: 'Sentinel_Classified_RF',
  scale: 20,
  region: roi,
  maxPixels: 1e13,
});

